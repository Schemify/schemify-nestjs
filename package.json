  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch ",
    "start:prod": "node dist/apps/schemify-nestjs/main",
    "start:all": "concurrently \"nest start api-gateway\" \"nest start schemify-nestjs\" \"nest start schemify-nestjs-consumer\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.config.ts",
    "test:e2e:schemify": "jest --selectProjects schemify-microservice-nestjs",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.13",
