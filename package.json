    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "protobufjs": "^7.4.0",
    "protoc-gen-ts": "^0.8.7",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
